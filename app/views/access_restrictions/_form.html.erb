<%= simple_form_for @access_restriction do |f| %>
  <%= f.association :database, label_method: :label, value_method: :id, include_blank: false,  label: 'Database' %>
  <%= f.association :access_restriction_type, label_method: :type_label, value_method: :id, include_blank: false,  label: 'Access Restriction Type' %>
  <%= f.input :note  %>
  <%= f.input :private_url  %>

  <%= f.button :submit %>
<% end %>
